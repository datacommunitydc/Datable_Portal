{
  "name": "Datable",
  "version": "0.0.1",
  "description": "Datable",
  "main": "js/app.js",
  "dependencies": {
    "classnames": "^2.1.3",
    "flux": "^2.0.1",
    "object-assign": "^1.0.0",
    "react": "^0.12.0"
  },
  "devDependencies": {
    "browserify": "^6.2.0",
    "envify": "^3.0.0",
    "jest-cli": "^0.4.3",
    "reactify": "^0.15.2",
    "uglify-js": "~2.4.15",
    "watchify": "^2.1.1",
    "lite-server": "^2.1.0"
  },
  "scripts": {
    "start": "concurrently \"npm run serve\" \"watchify -o js/bundle.js -v -d js/app.js\"",
    "build": "browserify . -t [envify --NODE_ENV production] | uglifyjs -cm > js/bundle.min.js",
    "test": "jest",
    "serve": "lite-server -c config/lite-server.json"
  },
  "author": "Dhaval Zala",
  "browserify": {
    "transform": [
      "reactify",
      "envify"
    ]
  },
  "jest": {
    "rootDir": "./js"
  }
}
